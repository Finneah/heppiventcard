{
  "name": "heppiventcard",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest --config=jest.config.js --coverage --updateSnapshot ",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "build-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "release-build": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/ && rm -rf android/app/src/main/res/drawable-* && rm -rf android/app/src/main/res/raw/* ",
    "build-ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ./ios",
    "refresh": "npm i && cd ios && pod install && cd ..",
    "pod": "cd ios && pod install && cd ..",
    "tcp": "source ~/.bash_profile && adb reverse tcp:8081 tcp:8081",
    "postinstall": "npx typesync",
    "postversion": "react-native-version",
    "tsc": "tsc",
    "clean": "rimraf build",
    "build": "yarn run clean && yarn run assets  && yarn run tsc ",
    "assets": "mkdir build && cp -r src/image build/image",
    "watch": "yarn run build -- -w ",
    "watchAndRunAndroid": "concurrently \"yarn run watch\" \"yarn run android\"",
    "buildRunAndroid": "yarn run build && yarn run watchAndRunAndroid ",
    "watchAndRunIOS": "concurrently \"yarn run watch\" \"yarn run ios\"",
    "buildRunIOS": "yarn run build && yarn run watchAndRunIOS ",
    "watchAndStart": "concurrently \"yarn run watch\" \"yarn run start\"",
    "buildAndStart": "yarn run build && yarn run watchAndStart"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.13.2",
    "@react-native-community/netinfo": "^5.9.10",
    "@react-navigation/core": "^5.14.4",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "@reduxjs/toolkit": "^1.6.0",
    "axios": "^0.21.1",
    "native-base": "^3.0.3",
    "react": "17.0.1",
    "react-native": "0.64.1",
    "react-native-camera": "^3.41.0",
    "react-native-i18n": "^2.0.15",
    "react-native-modal": "^11.5.6",
    "react-native-modal-patch": "https://github.com/HarvestProfit/react-native-modal-patch.git",
    "react-native-permissions": "^3.0.5",
    "react-native-qrcode-scanner": "^1.5.1",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^2.16.1",
    "react-native-svg": "^12.1.1",
    "react-native-vector-icons": "^7.1.0",
    "react-native-webview": "^11.0.2",
    "react-navigation": "^4.4.3",
    "react-navigation-stack": "^2.10.2",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "rn-fetch-blob": "^0.12.0",
    "rn-tourguide": "^2.7.1",
    "styled-components": "^5.3.0",
    "styled-system": "^5.1.5",
    "vasern": "^0.3.68"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@testing-library/react-native": "^7.2.0",
    "@types/babel__core": "^7.1.15",
    "@types/concurrently": "^6.2.1",
    "@types/eslint": "^7.28.0",
    "@types/grunt": "^0.4.27",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-native": "^0.64.12",
    "@types/react-native-i18n": "^2.0.0",
    "@types/react-native-vector-icons": "^6.4.8",
    "@types/react-redux": "^7.1.18",
    "@types/react-test-renderer": "^17.0.1",
    "@types/rimraf": "^3.0.1",
    "@types/rn-fetch-blob": "^1.2.2",
    "@types/styled-components": "^5.1.11",
    "@types/styled-system": "^5.1.12",
    "babel-jest": "^26.6.3",
    "better-docs": "^1.2.2",
    "concurrently": "^6.2.0",
    "eslint": "^7.14.0",
    "grunt": "^1.3.0",
    "grunt-jsdoc": "^2.4.1",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-native-gesture-handler": "^1.9.0",
    "react-test-renderer": "17.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.3",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "jest": {
    "preset": "react-native"
  }
}
